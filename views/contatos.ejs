<body class="bg-light form-signin">
    <div class="bg-info clearfix">

        <h5 class="text-center  font-italic"> Pagina Inicial</h5>
    </div>
    <h1 class="text-center font-weight-bold">Cadastrar Contatos</h1>

    <hr>

    <a href="/formContato"><button type="button" class="btn btn-info">Incluir</button></a>

    <table class="table table-borderless">
        <tr>
            <th scope="col">Id </th>
            <th scope="col">Nome </th>
            <th scope="col">E-mail </th>
            <th scope="col">Telefone </th>
            <th scope="col">Rede Social</th>
            <th scope="col">CPF</th>
        </tr>
        <% contato.forEach(function(item){ %>
        <tr>
            <th scope="row"><%= item.id %></th>
            <td><%= item.nome %></td>
            <td><%= item.email %></td>
            <td><%= item.telefone %></td>
            <td><%= item.redesocial %></td>
            <td><%= item.cpf %></td>

            <a href="/atualizarContato?id=<%= item.id %>"><button type="button"
                    class="btn btn-outline-info">Atualizar</button></a>
            </td>
            <td>
                <a href="/excluirContato?id=<%= item.id %>" onClick="return confirm('Confirma a exclusÃ£o?')"><button
                        type="button" class="btn btn-danger">Excluir</button></a>
            </td>
        </tr>
</body>
<% }) %>
</table>